sprite-image()
  background-image: url('images/sprite.png')
sprite-position(sprite)
  background-position: sprite[0] sprite[1]
sprite-width(sprite)
  width: sprite[2]
sprite-height(sprite)
  height: sprite[3]
sprite(sprite)
