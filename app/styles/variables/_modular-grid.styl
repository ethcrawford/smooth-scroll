/* Align self
------------------------------------------------*/
align-self = false // requires: column, default: false
align-self-list-default = 'auto' 'start' 'end' 'center' 'baseline' 'stretch' // default: ['auto' 'start' 'end' 'center' 'baseline' 'stretch']
//
align-self-xs = false // requires: [media-xs, column-xs], default: false
align-self-list-xs = 'auto' 'start' 'end' 'center' 'baseline' 'stretch' // default: ['auto' 'start' 'end' 'center' 'baseline' 'stretch']
